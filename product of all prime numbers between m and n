m = int(input("Enter first number (m): "))
n = int(input("Enter second number (n): "))
if m > n:
    print("Product of prime numbers between", m, "and", n, "is: 0")
else:
    product = 1
    prime = False 
    for num in range(m, n + 1):
        if num > 1: 
            is_prime = True
            for i in range(2, num):
                if num % i == 0:
                    is_prime = False
                    break
            if is_prime:
                product = product * num
                prime = True
    if prime:
        print("Product of prime numbers between", m, "and", n, "is:", product)
    else:
        print("No prime numbers between", m, "and", n)
