m = int(input("Enter first number (m): "))
n = int(input("Enter second number (n): "))

if m > n:
    print("Product of integers between", m, "and", n, "is: 0")
else:
    product = 1
    for i in range(m, n + 1):
        product = product * i
    print("Product of integers between", m, "and", n, "is:", product)
