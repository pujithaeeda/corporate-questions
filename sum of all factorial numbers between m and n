m = int(input("Enter first number (m): "))
n = int(input("Enter second number (n): "))

if m > n:
    print("Sum of factorials between", m, "and", n, "is: 0")
else:
    total = 0
    for i in range(m, n + 1):
        fact = 1
        for j in range(1, i + 1):
            fact = fact * j 
        total = total + fact
    print("Sum of factorials between", m, "and", n, "is:", total)
